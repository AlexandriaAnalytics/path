{
    "name": "path/sinapsis",
    "type": "project",
    "description": "Sinapsis",
    "keywords": [
        "path",
        "sinapsis"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "barryvdh/laravel-dompdf": "^2.0",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "filament/filament": "^3.1",
        "filament/spatie-laravel-settings-plugin": "^3.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^11",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.8",
        "maatwebsite/excel": "^3.1",
        "mercadopago/dx-php": "^3.0",
        "mpdf/mpdf": "^8.2",
        "sentry/sentry-laravel": "^4.2",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-data": "^4.5",
        "spatie/laravel-pdf": "^1.1",
        "spatie/laravel-permission": "^6.3",
        "spatie/laravel-ray": "^1.35",
        "srmklive/paypal": "~3.0",
        "stechstudio/filament-impersonate": "^3.8",
        "stripe/stripe-php": "^13.12",
        "ysfkaya/filament-phone-input": "^2.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.10",
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "nunomaduro/phpinsights": "^2.11",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
